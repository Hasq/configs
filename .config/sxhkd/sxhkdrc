											jumpapp '/opt/sublime_text_3/sublime_text' & \
											firefox --profile $HOME/.mozilla/firefox/stable.profile & \
											jumpapp spacefm & \
											jumpapp mysql-workbench & \
											jumpapp -c SmartGit -i SmartGit '/opt/smartgit/bin/smartgit.sh' & \
											jumpapp -c 'org-zaproxy-zap-ZAP' -i 'sun-awt-X11-XFramePeer' '/opt/zap/zap.sh' & \
											jumpapp -f '/usr/bin/skype'
super + Return
											terminator
super + alt + Escape
											/opt/scripts/killbymouse.sh
alt + F2
											/usr/bin/gmrun
alt + F3
											"$HOME/.config/dmenu/dmenu-bind.sh"
super + shift + g
											zenity --question --text 'smartgit'        && /opt/smartgit/bin/smartgit.sh
super + shift + q
											zenity --question --text 'mysql-workbench' && /usr/bin/mysql-workbench
super + shift + x
											zenity --question --text 'zap'             && zaproxy
super + shift + r
